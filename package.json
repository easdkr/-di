{
  "name": "typescript-di-container",
  "version": "1.0.0",
  "description": "typescript-di-container",
  "main": "bootstrap.ts",
  "scripts": {
    "start": "node dist/bootstrap.js",
    "start:dev": "NODE_ENV=development ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --trace-warnings --trace-uncaught -- bootstrap.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest --config jest.config.js",
    "prebuild": "rm -rf dist"
  },
  "author": "choie0423@naver.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}
